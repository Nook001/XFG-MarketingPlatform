<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.nook.infrastructure.persistence.mapper.StrategyMapper">

    <!-- Result Map for Award entity -->
    <resultMap id="StrategyResultMap" type="org.nook.infrastructure.persistence.po.StrategyPO">
        <id property="id" column="id" />
        <result property="strategyId" column="strategy_id" />
        <result property="strategyDesc" column="strategy_desc" />
        <result property="ruleModels" column="rule_models" />
        <result property="createTime" column="create_time" />
        <result property="updateTime" column="update_time" />
    </resultMap>


    <select id="getByStrategyId" resultMap="StrategyResultMap" parameterType="Long">
        SELECT *
        FROM strategy
        WHERE strategy_id = #{strategyId}
    </select>



</mapper>
